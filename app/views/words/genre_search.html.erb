<main>
  <div class="side-bar">
    <%= render "side_bar"%>
  </div>
  <div class="genre_search-bar">
    <div class="word__wrapper">
      <% @words.each do |word| %>
        <%= render partial: "word", locals: { word: word } %>
      <% end %>
    </div>
  </div>
</main>
<div class="home_icon">
  <a href="/words"><i class="fa fa-home fa-6x"></i></a>
</div>
  <% if user_signed_in? %>
  <div class="new_icon">
    <a href="/words/new"><i class="fas fa-camera fa-6x"></i></a>
  </div>
  <% end %>